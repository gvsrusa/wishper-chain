# Environment Variables Template
# Copy this file to .env and fill in your actual values

# Clerk Configuration
EXPO_PUBLIC_CLERK_PUBLISHABLE_KEY=pk_test_your_clerk_publishable_key_here

# Supabase Configuration (for database only)
EXPO_PUBLIC_SUPABASE_URL=your_supabase_project_url
EXPO_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key

# OAuth Configuration (for Clerk social login)
EXPO_PUBLIC_GOOGLE_WEB_CLIENT_ID=your_google_web_client_id.googleusercontent.com
